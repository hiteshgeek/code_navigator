{
  "name": "code-navigator",
  "displayName": "Code Navigator",
  "description": "Navigate to next/previous code block (function, class, trait, interface, etc.) using keyboard shortcuts.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "code-navigator.gotoNextBlockStart",
        "title": "Go to Next Block Start"
      },
      {
        "command": "code-navigator.gotoPreviousBlockStart",
        "title": "Go to Previous Block Start"
      },
      {
        "command": "code-navigator.gotoNextBlockEnd",
        "title": "Go to Next Block End"
      },
      {
        "command": "code-navigator.gotoPreviousBlockEnd",
        "title": "Go to Previous Block End"
      }
    ],
    "keybindings": [
      {
        "command": "code-navigator.gotoNextBlockStart",
        "key": "ctrl+alt+down",
        "when": "editorTextFocus"
      },
      {
        "command": "code-navigator.gotoPreviousBlockStart",
        "key": "ctrl+alt+up",
        "when": "editorTextFocus"
      },
      {
        "command": "code-navigator.gotoNextBlockEnd",
        "key": "ctrl+alt+pagedown",
        "when": "editorTextFocus"
      },
      {
        "command": "code-navigator.gotoPreviousBlockEnd",
        "key": "ctrl+alt+pageup",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.101.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "ts-loader": "^9.5.2",
    "webpack": "^5.99.7",
    "webpack-cli": "^6.0.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hiteshgeek/code_navigator.git"
  },
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#007acc",
    "theme": "dark"
  }
}
